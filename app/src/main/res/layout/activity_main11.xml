<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.appcompat.widget.SearchView
        android:id="@+id/main_4searchView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:iconifiedByDefault="false"
        android:queryHint="请输入搜索内容" >
    </androidx.appcompat.widget.SearchView>


    <TextView
        android:id="@+id/main11_edittext1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="100dp"
        android:clickable="true"
        android:text="关注"
        android:textSize="60sp"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/main11_edittext2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/main11_edittext1"
        android:layout_marginTop="100dp"
        android:layout_marginRight="90dp"
        android:text="发现"
        android:textColor="#FF7F50"
        android:clickable="true"
        android:textSize="60sp"
        android:textStyle="bold" />



    <View
        android:id="@+id/main11_view1"
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_below="@id/main11_edittext2"
        android:background="#000000">

    </View>
    <ImageView
        android:id="@+id/main11_pic1"
        android:scaleType="center"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/main11_view1"
        android:src="@mipmap/userpicture"
        />
    <ImageView
        android:id="@+id/main11_picfocus"
        android:scaleType="center"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/main11_view1"
        android:src="@mipmap/focus"
        android:layout_marginLeft="370dp"
        />
    <TextView
        android:id="@+id/main11_edittext3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/main11_edittext2"
        android:layout_toRightOf="@+id/main11_pic1"
        android:text="名称"
        android:textSize="35sp"
/>
    <TextView
        android:id="@+id/main11_edittext4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/main11_edittext3"
        android:layout_marginTop="20dp"
        android:text="操作..."
        android:clickable="true"
        android:textSize="60sp"
        android:textStyle="bold" />
    <ImageView
        android:id="@+id/main11_pic_collection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/main11_edittext4"
        android:src="@mipmap/collect"
        android:layout_marginTop="40dp"
        />
    <ImageView
        android:id="@+id/main11_pic_comment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/main11_edittext4"
        android:src="@mipmap/comment"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="40dp"
        />

    <ImageView
        android:id="@+id/main11_like"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/main11_edittext4"
        android:layout_marginLeft="370dp"
        android:layout_marginTop="40dp"
        android:src="@mipmap/like" />

    <View
        android:id="@+id/main11_view2"
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_below="@id/main11_pic_comment"
        android:background="#000000">
        android:layout_marginTop="40dp"

    </View>

    <TextView
        android:id="@+id/main11_edittext5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/main11_view2"
        android:layout_marginTop="10dp"
        android:text="..."
        android:textSize="60sp"
        />



    <Button
        android:id="@+id/main11_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="返回"
        android:textSize="48sp" />



    <Button
        android:id="@+id/main11_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:text="消息"
        android:textSize="48sp"/>



</RelativeLayout>
