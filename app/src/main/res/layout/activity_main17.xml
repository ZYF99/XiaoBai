<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView

    android:id="@+id/main17_edittext9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="返回"
    android:clickable="true"
    android:textSize="30sp"
    android:layout_marginTop="15dp"
    android:layout_marginLeft="10dp"
    android:textStyle="bold"
    />
<TextView
android:id="@+id/main17_edittext1"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
    android:textColor="#FF7F50"
android:text="设置"
android:clickable="true"
android:layout_toRightOf="@id/main17_edittext9"
android:layout_marginLeft="100dp"
android:textSize="30sp"
android:layout_marginTop="40dp"
android:textStyle="bold"
    />

    <TextView

        android:id="@+id/main17_edittext2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="账号与安全"
        android:clickable="true"
        android:layout_below="@+id/main17_edittext1"
        android:textSize="30sp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="30dp"
        android:textStyle="bold"
        />
    <TextView

        android:id="@+id/main17_edittext3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="隐私设置"
        android:clickable="true"
        android:layout_below="@+id/main17_edittext2"
        android:textSize="30sp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:textStyle="bold"
        />
    <TextView

        android:id="@+id/main17_edittext4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="个人资料"
        android:clickable="true"
        android:layout_below="@+id/main17_edittext3"
        android:textSize="30sp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:textStyle="bold"
        />
    <TextView

        android:id="@+id/main17_edittext5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="退出登录"
        android:clickable="true"
        android:layout_below="@+id/main17_edittext4"
        android:textSize="30sp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:textStyle="bold"
        />



</RelativeLayout>